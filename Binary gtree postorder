class Solution {
    public List<Integer> postorderTraversal(TreeNode root) {
           List<Integer> ans= new ArrayList<>();
           f(root,ans);
           return ans;    
    }
    public void f(TreeNode root,List<Integer>li){
        if(root!=null){
            f(root.left,li);
            f(root.right,li);
            li.add(root.val);
        }
    }
}
