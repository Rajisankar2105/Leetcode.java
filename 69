class Solution{
    public int maximum69Number(int num){
        String numStr=Integer.toString(num);
        StringBuilder sb=new StringBuilder(numStr);   
        for(int i=0;i<numStr.length();i++){
            if(numStr.charAt(i)=='6'){
                sb.setCharAt(i,'9');
                break;
       }
        }
        return Integer.parseInt(sb.toString());
    }
}
    
